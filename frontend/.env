# URL del backend para desarrollo
REACT_APP_API_URL=http://localhost:8000/api

# Configuración del entorno (development, production, test)
REACT_APP_ENVIRONMENT=development

# Variables de configuración para notificaciones o UI
REACT_APP_NOTIFICATIONS_ENABLED=true

# Claves o identificadores públicos
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Puerto en el que el servidor de desarrollo de React escucha
PORT=3000

